<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5level >>> %d{dd:MM:YYYY HH:mm:ss} [%t] %logger{36} - %msg%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="INFO">
<!--        <Root level="ALL">-->
<!--        <Root level="DEBUG">-->
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
